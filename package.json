{
  "name": "amp-dev",
  "version": "1.0.2",
  "description": "The successor of the former ampproject.org: the all-new amp.dev",
  "repository": "git@github.com:ampproject/docs.git",
  "main": "serve.js",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "develop": "cd platform && node build.js",
    "lint": "npm-run-all lint:* --aggregate-output --parallel",
    "lint-disabled:grow": "cd platform && node lib/pipeline/growReferenceChecker.js",
    "lint:node": "eslint \"**/*.js\" --ignore-path ./.gitignore",
    "fix:node": "eslint \"**/*.js\" --fix --ignore-path ./.gitignore",
    "build:boilerplate": "cd boilerplate && node build.js",
    "build:local": "NODE_ENV=local npm run build:site",
    "build:playground": "cd playground && webpack --config webpack.config.js --mode production --optimize-minimize --progress",
    "build:site": "cd platform && node build.js --import --clean-samples",
    "build:staging": "NODE_ENV=staging npm-run-all build:site build:boilerplate build:playground --aggregate-output --parallel",
    "build:prod": "NODE_ENV=production npm-run-all build:site build:boilerplate build:playground --aggregate-output --parallel",
    "deploy:staging": "gcloud app deploy --project=amp-dev-staging --quiet",
    "deploy:prod": "gcloud app deploy --project=amp-dev-230314 --quiet",
    "log:staging": "gcloud app logs tail -s default --project amp-dev-staging",
    "posttest": "npm run lint",
    "start:local": "cd platform && NODE_ENV=local node serve.js",
    "start:playground": "cd playground && webpack-dev-server --mode development",
    "start": "cd platform && NODE_ENV=production node serve.js",
    "test": "npm-run-all test:* --aggregate-output --parallel",
    "test:playground": "babel-node playground/spec/run.js"
  },
  "keywords": [
    "amphtml",
    "framework",
    "documentation"
  ],
  "author": "The AMP HTML Authors",
  "license": "ISC",
  "signale": {
    "displayTimestamp": true
  },
  "_moduleAliases": {
    "@lib": "platform/lib"
  },
  "dependencies": {
    "amp-toolbox-cors": "0.2.1",
    "amp-toolbox-optimizer": "0.4.6",
    "amp-toolbox-optimizer-express": "0.5.0",
    "amp-toolbox-runtime-version": "0.2.3",
    "cheerio": "1.0.0-rc.2",
    "compression": "1.7.3",
    "express": "4.16.4",
    "js-yaml": "3.12.1",
    "module-alias": "2.1.0",
    "mri": "1.1.4",
    "node-fetch": "2.3.0",
    "shrink-ray-current": "^3.0.2",
    "signale": "1.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/node": "7.2.2",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "7.3.1",
    "@babel/runtime": "^7.3.1",
    "@octokit/rest": "16.15.0",
    "absolutify": "0.1.0",
    "amp-by-example": "0.2.5",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "clean-css": "4.2.1",
    "clean-webpack-plugin": "1.0.1",
    "closure-webpack-plugin": "2.0.0-rc.17",
    "codemirror": "5.43.0",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "2.1.0",
    "del": "3.0.0",
    "eslint": "5.13.0",
    "eslint-config-google": "0.12.0",
    "file-loader": "3.0.1",
    "google-closure-compiler": "20190121.0.0",
    "got": "9.6.0",
    "gulp": "4.0.0",
    "gulp-once": "2.0.3",
    "gulp-sass": "4.0.2",
    "gulp-strip-css-comments": "2.0.0",
    "handlebars": "4.1.0",
    "handlebars-loader": "1.7.1",
    "handlebars-registrar": "1.5.2",
    "highlight.js": "^9.14.2",
    "html-loader": "0.5.5",
    "html-minifier": "3.5.21",
    "html-webpack-inline-source-plugin": "1.0.0-beta.2",
    "html-webpack-plugin": "4.0.0-beta.4",
    "http-proxy": "1.17.0",
    "http-proxy-response-rewrite": "0.0.1",
    "jasmine": "3.3.1",
    "js-beautify": "1.8.9",
    "keymaster": "1.6.2",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "4.11.0",
    "npm-run-all": "4.1.5",
    "octonode": "0.9.5",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "preload-webpack-plugin": "3.0.0-beta.3",
    "rcs-core": "2.4.6",
    "recursive-search": "1.0.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "through2": "3.0.0",
    "url-loader": "1.1.2",
    "url-search-params": "1.1.0",
    "wade": "0.3.3",
    "webpack": "4.29.3",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14",
    "write": "1.0.3",
    "write-yaml": "1.0.0"
  },
  "sideEffects": [
    "./playground/src/request-idle-callback/base.js",
    "./playground/src/service-worker/base.js",
    "./playground/src/event-listener-options/base.js",
    "*.css",
    "*.scss"
  ]
}
